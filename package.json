{
  "name": "edu_kinder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "postinstall": "npm run copy-idsk-assets",
    "copy-idsk-assets": "npm run copy-idsk-govuk && npm run copy-idsk-idsk && npm run copy-idsk-idsk-dist",
    "copy-idsk-govuk": "copyfiles -u 4 \"node_modules/@id-sk/frontend/govuk/assets/**/*\" public/idsk-assets",
    "copy-idsk-idsk": "copyfiles -u 4 \"node_modules/@id-sk/frontend/idsk/assets/**/*\" public/idsk-assets",
    "copy-idsk-idsk-dist": "copyfiles -u 5 \"node_modules/@id-sk/frontend/idsk/dist/assets/**/*\" public/idsk-assets"
  },
  "dependencies": {
    "@id-sk/frontend": "^3.0.0-beta.0",
    "cheerio": "^1.1.2",
    "next": "15.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sass": "^1.93.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "copyfiles": "^2.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "typescript": "^5"
  }
}
